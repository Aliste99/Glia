<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/t1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="Shared" />

        <TextView
            android:id="@+id/shared"
            android:layout_width="197dp"
            android:layout_height="wrap_content"
            android:elevation="0dp"
            android:text="TextView" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/t2"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="Reached total" />

        <TextView
            android:id="@+id/reached_total"
            android:layout_width="197dp"
            android:layout_height="wrap_content"
            android:elevation="0dp"
            android:text="TextView" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/t3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="Reached unique" />

        <TextView
            android:id="@+id/reached_unique"
            android:layout_width="197dp"
            android:layout_height="wrap_content"
            android:elevation="0dp"
            android:text="TextView" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/t4"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="Likes" />

        <TextView
            android:id="@+id/likes_count"
            android:layout_width="197dp"
            android:layout_height="wrap_content"
            android:elevation="0dp"
            android:text="TextView" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/t5"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="Loves" />

        <TextView
            android:id="@+id/loves_count"
            android:layout_width="197dp"
            android:layout_height="wrap_content"
            android:elevation="0dp"
            android:text="TextView" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/t6"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="HaHa" />

        <TextView
            android:id="@+id/haha_count"
            android:layout_width="197dp"
            android:layout_height="wrap_content"
            android:elevation="0dp"
            android:text="TextView" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/t7"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="Wow" />

        <TextView
            android:id="@+id/wow_count"
            android:layout_width="197dp"
            android:layout_height="wrap_content"
            android:elevation="0dp"
            android:text="TextView" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/t8"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="Sad" />

        <TextView
            android:id="@+id/sad_count"
            android:layout_width="197dp"
            android:layout_height="wrap_content"
            android:elevation="0dp"
            android:text="TextView" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/t9"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="Angry" />

        <TextView
            android:id="@+id/angry_count"
            android:layout_width="197dp"
            android:layout_height="wrap_content"
            android:elevation="0dp"
            android:text="TextView" />
    </LinearLayout>

</LinearLayout>